package practice;

public class LogicPrac {

	public static void main(String[] args) {
		
		int a = 1;
		int b = 2;
		int c = 3;
		int sum = 0;
		int sum2 = 0;
		
		
		if( a < b && a < c) { // a<b가 참이면 다음 값으로 이동 -> a<c 
			sum = a + c;
		System.out.println(sum);
			
		}
		
		if( a == c || a < b) { // a==c 가 거짓이면 다음값으로 이동 
			sum2 = a + c;
		System.out.println(sum2);
		}
		
		
		
	}
	
}
